<html xmlns="http://www.w3.org/1999/xhtml">
  <body onLoad="MM_preloadImages('/assets/static/graphics/logo_pogo.png','/assets/client3_on.gif','/assets/client1_on.gif','/assets/client2_on.gif','/assets/client4_on.gif','/assets/footer-logo_on.png')" >
    <div class="client_dashboard_box">
      <h2 class="red_color">Client dashboard</h2>
      <%= nested_form_for(@client, :url => "/create_client") do |f|%>
        <%= f.hidden_field :user_id, :value => current_user.id %>
        <div>
          <% if @client.errors.any? %>
            <div id="error_explanation" class="red_color">
              <h2 class="red_color"><%= pluralize(@client.errors.count, "error") %> prohibited this user from being saved:</h2>

              <ul>
                <% @client.errors.full_messages.each do |msg| %>
                  <li><%= msg %></li>
                <% end %>
              </ul>
            </div>
          <% end %>
