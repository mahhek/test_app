            HSS Partner *<br/>
            <%= f.text_field :hss_partner , :class => "big_text_field"%>
          </div>
          <div class="float_left">
            Client Name * <br/>
            <%= f.text_field :client_name , :class => "big_text_field"%>
          </div>
          <div class="float_left">
            Street Address <br/>
            <%= f.text_field :street_address1 , :class => "big_text_field"%>
          </div>
          <div class="float_left">
            Street Address 2 <br/>
            <%= f.text_field :street_address2 , :class => "big_text_field"%>
          </div>
          <div class="float_left">
            City <br/>
            <%= f.text_field :city , :class => "big_text_field"%>
          </div>
          <div class="float_left">
            State/Province <br/>
            <%= f.select :province , options_for_select(["AL", "AK", "AZ", "AR", "CA", "CO", "CT", "DE", "FL", 'GA', 'HI','ID','IL','IN','IA','KS','KY','LA','ME','MD','MA','MI','MN','MS','MO','MT','NE','NV','NH','NJ','NM','NY','NC','ND','OH','OK','OR','PA','RI','SC','SD','TN','TX','UT','VT','VA','WA','WV','WI','WY'], @client.province), :class => "big_text_field"%>
          </div>
          <div class="float_left">
            Zip Code <br/>
            <%= f.text_field :zip_code , :class => "big_text_field"%>
          </div>
          <div class="float_left">
            Contact Name<br/>
            <%= f.text_field :contact , :class => "big_text_field"%>
          </div>
          <div class="float_left">
            Contact Phone <br/>
            <%= f.text_field :contact_phone , :class => "big_text_field"%>
            <br/>
            <i>Ex: 111-222-3456</i>
          </div>
          <div class="float_left">
            Contact Email<br/>
            <%= f.email_field :contact_email , :class => "big_text_field"%>
          </div>
          <div class="float_left">
            Covered Entity <br/>
            <%= f.text_field :covered_entity , :class => "big_text_field"%>
          </div>
          <div class="float_left">
            Covered Entity Address <br/>
            <%= f.text_field :covered_entity_address1 , :class => "big_text_field"%>
          </div>
          <div class="float_left">
            Covered Entity Address 2 <br/>
            <%= f.text_field :covered_entity_address2 , :class => "big_text_field"%>
          </div>
          <div class="float_left">
            Covered Entity City <br/>
            <%= f.text_field :covered_entity_city , :class => "big_text_field"%>
          </div>
          <div class="float_left">
            Covered Entity State <br/>
            <%= f.select :covered_entity_state, options_for_select(["AL", "AK", "AZ", "AR", "CA", "CO", "CT", "DE", "FL", 'GA', 'HI','ID','IL','IN','IA','KS','KY','LA','ME','MD','MA','MI','MN','MS','MO','MT','NE','NV','NH','NJ','NM','NY','NC','ND','OH','OK','OR','PA','RI','SC','SD','TN','TX','UT','VT','VA','WA','WV','WI','WY'], @client.covered_entity_state) , :class => "big_text_field"%>
