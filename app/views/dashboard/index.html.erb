          <div class="float_left">
            Covered Entity Zip Code <br/>
            <%= f.text_field :covered_entity_zip_code , :class => "big_text_field"%>
          </div>
          <div class="float_left">
            Information security officer <br/>
            <%= f.text_field :info_security_officer , :class => "big_text_field"%>
          </div>
          <div class="float_left">
