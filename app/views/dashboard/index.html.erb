              </div>
              <div class="float_left"  style="width:100%;">
                Name <br/>
                <%= task_form.text_field :name , :class => "big_text_field"%>
              </div>
              <div class="float_left"  style="width:100%;">
                Title <br/>
                <%= task_form.text_field :title , :class => "big_text_field"%>
              </div>
              <div class="float_left" style="width:100%;">
                Phone <br/>
                <%= task_form.text_field :phone , :class => "big_text_field"%>
                <br/>
            <i>Ex: 111-222-3456</i>
              </div>
              <div class="float_left" style="width:100%;">
                Email <br/>
                <%= task_form.email_field :email , :class => "big_text_field"%>
              </div>
              <div class="float_left" style="width:100%;">
